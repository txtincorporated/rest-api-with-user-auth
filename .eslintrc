{
   "settings": {
    "ecmascript": 6
  },
  "ecmaFeatures": {
  },
  "env": {
    "es6": true,
    "node": true,
    "mocha": true,
    "browser": true,
    "jquery": true
  },
  "rules": {
    "semi": 1,
    "strict": 0,
    "indent": [2, 2],
    "quotes": [1, "single"],
    "no-multi-spaces": [1, {
      "exceptions": {
        "VariableDeclarator": true,
        "FunctionExpression": true
      }
    }],
    "key-spacing": [0, {"align": "value"}],
    "no-underscore-dangle": 0,
    "no-unused-vars": 1
  }
}
